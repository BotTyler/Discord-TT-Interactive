{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
  },
  "include": ["src/**/*.ts", "./environment.d.ts", "./src/shared/LoadDataInterfaces.ts", "./src/shared/PositionInterface.ts"],
  "exclude": ["node_modules"]
}
